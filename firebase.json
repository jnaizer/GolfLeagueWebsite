{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "dist/golf-league-website",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    } ]
  },
  "database": {
    "rules": "database.rules.json"
  }
}
